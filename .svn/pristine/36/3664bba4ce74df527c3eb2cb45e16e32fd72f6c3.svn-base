#import "First-Cell.h"

@implementation First_Cell

-(instancetype)initWithStyle:(UITableViewCellStyle)style reuseIdentifier:(NSString *)reuseIdentifier
{
    self = [super initWithStyle:style reuseIdentifier:reuseIdentifier];
    if (self) {
        self.titleLab = [[UILabel alloc] init];
        self.titleLab.font = [UIFont fontWithName:@"Microsoft Yahei UI" size:15];
        [self.contentView addSubview:self.titleLab];
        
        
        self.authorLab = [[UILabel alloc] init];
        self.authorLab.font = [UIFont fontWithName:@"Microsoft Yahei UI" size:12];
        [self.contentView addSubview:self.authorLab];
        
        
        self.lookTimeLab = [[UILabel alloc] init];
        self.lookTimeLab.font = [UIFont fontWithName:@"Microsoft Yahei UI" size:12];
        [self.contentView addSubview:self.lookTimeLab];
    }
    return self;
}

@end
